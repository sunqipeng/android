<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-permission android:name="android.permission.SEND_SMS"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_SMS"></uses-permission>
    <uses-permission android:name="android.permission.VIBRATE"></uses-permission>
    <uses-permission android:name="android.permission.ACCOUNT_MANAGER"></uses-permission>
    <application android:label="@string/app_name" android:icon="@drawable/icon">

        <activity android:name="MyActivity"
                  android:label="@string/app_name">
            <intent-filter android:priority="999999">
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
                <category android:name="android.intent.category.HOME"></category>
            </intent-filter>
        </activity>
        <activity android:name=".second"/>

        <service android:name=".AppService" android:label="service"/>
        <service android:name=".AppIntentService"></service>
        <service android:name=".BindService"/>

        <receiver android:name=".AppReceiver">
            <intent-filter>
                <action android:name="edu.sun.android.BC"></action>
            </intent-filter>
        </receiver>
    </application>
</manifest> 
